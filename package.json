  "scripts": {
    "dev": "npm-run-all --parallel dev:web dev:server",
    "dev:web": "webpack-dev-server --open --config ./build/webpack.dev.js",
    "dev:admin1": "cross-env NODE_ENV=development node ./parcel-admin.js",
    "dev:web1": "parcel ./src/styles/index.scss --out-dir ./static/app/css",
    "dev:web:admin": "parcel ./src/styles/admin.scss --out-dir ./static/app/css",
    "dev:server": "nodemon --watch server/* server/app.js",
    "build": "rimraf ./static/app && webpack --config ./build/webpack.prod.js"
  }
