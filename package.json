{
  "name": "react-blog",
  "version": "3.0.0",
  "license": "MIT",
  "scripts": {
    "start": "razzle start",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "cp": "node ./scripts/cp.js",
    "start:prod": "cross-env NODE_ENV=production node build/server.js"
  },
  "dependencies": {
    "@emotion/core": "^10.0.4",
    "@emotion/styled": "^10.0.4",
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "dayjs": "^1.7.7",
    "emotion": "^10.0.6",
    "emotion-server": "^10.0.6",
    "express": "^4.16.4",
    "polished": "^2.3.1",
    "query-string": "^6.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^1.5.0",
    "shelljs": "^0.8.3",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/mockjs": "^1.0.2",
    "@types/node": "10.12.12",
    "@types/query-string": "^6.1.1",
    "@types/react": "~16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-config": "^1.1.0",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "@types/validator": "^9.4.3",
    "@types/webpack-env": "^1.13.6",
    "http-proxy-middleware": "^0.19.1",
    "mockjs": "^1.0.1-beta3",
    "razzle": "^2.4.0",
    "razzle-plugin-typescript": "^2.4.0",
    "shelljs": "^0.8.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  }
}
