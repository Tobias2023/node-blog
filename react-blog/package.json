{
  "name": "node-blog-web",
  "version": "2.0.0",
  "description": "A simple and whole react blog web",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel dev:client dev:server",
    "dev:client": "webpack-dev-server --open --config ./build/webpack.dev.js --hide-modules",
    "dev:server": "cross-env NODE_ENV=development node server/server.js",
    "build:client": "webpack --config ./build/webpack.prod.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.js --progress --hide-modules",
    "build": "npm-run-all --parallel build:client build:server",
    "start": "cross-env NODE_ENV=production node server/server.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cross-env": "^5.1.6",
    "dayjs": "^1.6.5",
    "express": "^4.16.3",
    "query-string": "^6.1.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "autoprefixer": "^8.5.2",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.3",
    "babel-plugin-import": "^1.7.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.5.2",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^0.28.11",
    "global": "^4.3.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "lru-cache": "^4.1.3",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "postcss-loader": "^2.1.5",
    "prettier": "1.13.3",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "validator": "^10.2.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.2",
    "@babel/runtime": "^7.0.0-beta.51",
    "babel-polyfill": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2"
  }
}
